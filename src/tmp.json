{
  "destination_number": [
    "380442252540",
    "380442252520"
  ],
  "domain": "nrg.bpmonline.com",
  "callflow": [

    {
      "set": ["ringback=$${ru-ring}", "transfer_ringback=$${hold_music}",
              "continue_on_fail=true", "hangup_after_bridge=true", "ignore_early_media=true"]
    },

    {
      "httpRequest": { "url": "https://nrg.bpmonline.com/0/ServiceModel/GetCallerOwnerService.svc/" }
    },
    {
      "if" : {
        "expression" : "1==1",
        "then" : [{
                    "bridge" : {
                      "parameters": ["call_timeout=10"],
                      "endpoints": [{ "name": "${owner_caller_id_number}", "type": "user" }]
                    }
                  }]
      }
    },
    {
      "goto": "default:nrg_in"
    }
  ],
  "version": 2
}